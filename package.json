{
  "name": "timer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run format && npm run lint && tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src",
    "type-check": "tsc --noEmit",
    "check-all": "npm run type-check && npm run lint",
    "format": "prettier --write src | grep -v \"unchanged\" | xargs",
    "format-verbose": "prettier --write src",
    "test": "jest --passWithNoTests",
    "prepare": "husky"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@types/jest": "^29.5.14",
    "eslint": "^9.25.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.5.3",
    "prettier-plugin-jsdoc": "^1.3.2",
    "ts-jest": "^29.3.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.31.1",
    "vite": "^6.3.1",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.5",
    "tailwindcss": "^4.1.5"
  }
}
